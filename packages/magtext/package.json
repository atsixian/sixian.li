{
  "name": "magtext",
  "version": "0.0.2",
  "description": "Magnetic hover effect for your text",
  "author": "Sixian<https://github.com/atsixian>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "unbuild",
    "lint": "eslint src/**/*",
    "dev": "unbuild --stub",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run check-types && unbuild --minify"
  },
  "peerDependencies": {
    "framer-motion": "^10.12.16",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "eslint-config-custom": "workspace:*",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0"
  },
  "keywords": [
    "react",
    "framer-motion",
    "animation",
    "text effect"
  ],
  "publishConfig": {
    "access": "public"
  }
}
